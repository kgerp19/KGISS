FROM            Company
WHERE        (CompanyId = @CompanyId)</CommandText></Query><Fields><Field Name="Name"><DataField>Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Address"><DataField>Address</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyLogo"><DataField>CompanyLogo</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="AssetDSet"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter></QueryParameters><CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

	SELECT * FROM (SELECT  Head2.AccName as AccName, 
						  SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount) as Balance, 
						  Head2.AccCode as AccCode 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                    WHERE Head1.AccCode = '1' 
					And Head2.AccCode &lt;&gt; '13' 
					and Voucher.CompanyId = @CompanyId
					AND Voucher.IsActive = 1
					AND Voucher.IsSubmit = 1
                    and VoucherDetail.IsActive = 1
                    and Voucher.VoucherDate  &lt;=  @FromDate
				
                    GROUP BY Head2.AccName, 
					         Head2.Id, 
							 Head2.AccCode) AS A
   WHERE A.Balance &lt;&gt; 0
   order by A.AccCode</CommandText></Query><Fields><Field Name="AccName"><DataField>AccName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="AccCode"><DataField>AccCode</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CurrentAssetDSet"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter></QueryParameters><CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

	
	SELECT * 
	FROM (SELECT  Head4.AccName AS AccName, 
				 SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount) AS Balance, 
				 Head4.AccCode AS AccCode 
	      FROM VoucherDetail
           JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
           JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
           JOIN Head5 ON HeadGL.ParentId = Head5.Id
           JOIN Head4 ON Head5.ParentId = Head4.Id
           JOIN Head3 ON Head4.ParentId = Head3.Id
           JOIN Head2 ON Head3.ParentId = Head2.Id
          WHERE Head2.AccCode = '13' 
		        AND Voucher.CompanyId =  @CompanyId 
				AND Voucher.IsActive = 1
					AND Voucher.IsSubmit = 1
				AND VoucherDetail.IsActive = 1
                AND Voucher.VoucherDate  &lt;=  @FromDate
          GROUP BY Head4.AccName, Head4.Id, Head4.AccCode) AS A
   WHERE A.Balance &lt;&gt; 0 
   ORDER BY A.AccCode</CommandText></Query><Fields><Field Name="AccName"><DataField>AccName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="AccCode"><DataField>AccCode</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CapitalLiabilityDSet"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @FromDate DATE
   SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	SELECT * FROM (SELECT Head2.AccName as AccName, 
						  SUM(VoucherDetail.CreditAmount)- SUM(VoucherDetail.DebitAmount) as Balance, 
						  Head2.AccCode as AccCode 
	               FROM VoucherDetail
                   JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                   JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                   JOIN Head5 on HeadGL.ParentId = Head5.Id
                   JOIN Head4 on Head5.ParentId = Head4.Id
                   JOIN Head3 on Head4.ParentId = Head3.Id
                   JOIN Head2 on Head3.ParentId = Head2.Id
                   JOIN Head1 on Head2.ParentId = Head1.Id

                   WHERE Head1.AccCode = '2' 
				         AND Head2.AccCode &lt;&gt; '24'
				         AND Head4.AccCode &lt;&gt; '2101001'

						 AND Voucher.CompanyId = @CompanyId
						 AND Voucher.IsActive = 1
					AND Voucher.IsSubmit = 1
						 AND VoucherDetail.IsActive =1
						
                         AND Voucher.VoucherDate &lt;= @FromDate
						
                   GROUP BY Head2.AccName,  Head2.AccCode
				   Union All
				   SELECT Head5.AccName as AccName, 
						  SUM(VoucherDetail.CreditAmount)- SUM(VoucherDetail.DebitAmount) as Balance, 
						  Head5.AccCode as AccCode 
	               FROM VoucherDetail
                   JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                   JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                   JOIN Head5 on HeadGL.ParentId = Head5.Id
                   JOIN Head4 on Head5.ParentId = Head4.Id
                   JOIN Head3 on Head4.ParentId = Head3.Id
                   JOIN Head2 on Head3.ParentId = Head2.Id
                   JOIN Head1 on Head2.ParentId = Head1.Id

                   WHERE Head4.AccCode = '2101001'

						 AND Voucher.CompanyId = @CompanyId
						 AND Voucher.IsActive = 1
					AND Voucher.IsSubmit = 1
						 AND VoucherDetail.IsActive =1
						
                         AND Voucher.VoucherDate &lt;= @FromDate
						
                   GROUP BY Head5.AccName,  Head5.AccCode
				   
				   
				   ) AS A
   WHERE A.Balance &lt;&gt; 0
   ORDER BY A.AccCode</CommandText></Query><Fields><Field Name="AccName"><DataField>AccName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="AccCode"><DataField>AccCode</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CurrentLiabilitiesDSet"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105)) 

	SELECT * FROM (SELECT Head5.AccName AS AccName, 
						  (SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount)) AS Balance, 
						  Head5.AccCode AS AccCode 
	               FROM VoucherDetail
                    JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 ON HeadGL.ParentId = Head5.Id
                    JOIN Head4 ON Head5.ParentId = Head4.Id
                    JOIN Head3 ON Head4.ParentId = Head3.Id
                    JOIN Head2 ON Head3.ParentId = Head2.Id
                   WHERE Head2.AccCode = '24' 
				         AND Voucher.CompanyId =  @CompanyId
                         AND VoucherDetail.IsActive = 1
						 AND Voucher.IsActive = 1
						 AND Voucher.IsSubmit =1
                         AND Voucher.VoucherDate &lt;= @FromDate 
                   GROUP BY Head5.AccName,  Head5.AccCode) AS A
   WHERE A.Balance &lt;&gt; 0
   ORDER BY A.AccCode</CommandText></Query><Fields><Field Name="AccName"><DataField>AccName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="AccCode"><DataField>AccCode</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="ProfitLoss"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter></QueryParameters><CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	SELECT
	Isnull((SELECT 
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END) as Balance 
	FROM VoucherDetail
     JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
     JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
     JOIN Head5 on HeadGL.ParentId = Head5.Id
     JOIN Head4 on Head5.ParentId = Head4.Id
     JOIN Head3 on Head4.ParentId = Head3.Id
     JOIN Head2 on Head3.ParentId = Head2.Id
     JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '3'
 and Head2.CompanyId =  @CompanyId 
 and Voucher.VoucherDate  &lt;=  @FromDate
 AND Voucher.IsActive =1
 AND Voucher.IsSubmit = 1
 AND VoucherDetail.IsActive =1
 Group By Head1.DrIncrease),0)
 -
 IsNull((SELECT 
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END) as Balance 
	FROM VoucherDetail
     JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
     JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
     JOIN Head5 on HeadGL.ParentId = Head5.Id
     JOIN Head4 on Head5.ParentId = Head4.Id
     JOIN Head3 on Head4.ParentId = Head3.Id
     JOIN Head2 on Head3.ParentId = Head2.Id
     JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '4' 
 and Voucher.CompanyId = @CompanyId 
 AND Voucher.IsActive =1
 AND Voucher.IsSubmit = 1
 AND VoucherDetail.IsActive =1
 and Voucher.VoucherDate  &lt;=  @FromDate 
 Group By Head1.DrIncrease),0) as Balance </CommandText></Query><Fields><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field></Fields></DataSet><DataSet Name="SignatoryDataSet"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter><QueryParameter Name="@ReportCategoryId"><Value>=Parameters!ReportCategoryId.Value</Value></QueryParameter></QueryParameters><CommandType>StoredProcedure</CommandType><CommandText>AccountingApprovalSignatory</CommandText></Query><Fields><Field Name="ApprovalStatus"><DataField>ApprovalStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ApprovedDate"><DataField>ApprovedDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="SignatoryName"><DataField>SignatoryName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FinalStatus"><DataField>FinalStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="SignatoryType"><DataField>SignatoryType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OrderBy"><DataField>OrderBy</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ReportStatus"><DataField>ReportStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>4in</Width></TablixColumn><TablixColumn><Width>2in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.25in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox2"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>PROPERTIES &amp; ASSETS</Value><Style><FontFamily>Times N