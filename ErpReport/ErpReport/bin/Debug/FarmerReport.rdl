<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><am:AuthoringMetadata xmlns:am="http://schemas.microsoft.com/sqlserver/reporting/authoringmetadata"><am:CreatedBy><am:Name>SSDTRS</am:Name><am:Version>17.6.33723.286</am:Version></am:CreatedBy><am:UpdatedBy><am:Name>SSDTRS</am:Name><am:Version>17.6.33723.286</am:Version></am:UpdatedBy><am:LastModifiedTimestamp>2024-01-16T05:40:40.5000589Z</am:LastModifiedTimestamp></am:AuthoringMetadata><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="CustomerDS"><DataSourceReference>ErpDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>de237ddf-2ebf-4078-b156-c3363cfa7481</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="FarmerDSet"><Query><DataSourceName>CustomerDS</DataSourceName><QueryParameters><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter></QueryParameters><CommandText>
SELECT        c.Name as Company,
			  (select Url from UrlInfo where UrlType=1)+'Images/Logo/'+ c.CompanyLogo  as CompanyLogo,
              c.[Address] as CompanyAddress,
              c.Phone  as CompanyPhone,
              c.Email  as CompanyEmail,
              c.ShortName  as CompanyName,
			  f.Name FarmarName,
			  f.Address,
			  f.CompanyId,
			  f.CreatedBy,
			  f.DateofBirth,
			  f.District,
			  f.Division,
			  f.Phone,
			  f.NationalId,
			  f.Remarks,
			  f.Spouse,
			  f.Upzila,
			  f.Village,
			  e.Name OfficerName,
			  e.EmployeeId,
			  z.Name ZoneName
             
FROM          Erp.Farmer f
JOIN Company c on f.CompanyId = c.CompanyId
left join Employee e on f.OfficerId = e.Id
left join Erp.Zone z on f.ZoneId = z.ZoneId

where         f.IsActive = 1 and f.CompanyId= @CompanyId</CommandText></Query><Fields><Field Name="Company"><DataField>Company</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyName"><DataField>CompanyName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyLogo"><DataField>CompanyLogo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyAddress"><DataField>CompanyAddress</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ZoneName"><DataField>ZoneName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyPhone"><DataField>CompanyPhone</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FarmarName"><DataField>FarmarName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyEmail"><DataField>CompanyEmail</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CompanyId"><DataField>CompanyId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="CreatedBy"><DataField>CreatedBy</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DateofBirth"><DataField>DateofBirth</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="Address"><DataField>Address</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="District"><DataField>District</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Phone"><DataField>Phone</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Division"><DataField>Division</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="NationalId"><DataField>NationalId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Spouse"><DataField>Spouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Upzila"><DataField>Upzila</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Village"><DataField>Vill