    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105)) 

	SELECT * FROM (SELECT Head3.Id, 
	                      Head3.AccName AS AccName, 
						  (SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount)) AS Balance, 
						  Head3.OrderNo AS OrderNo 
	               FROM VoucherDetail
                    JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 ON HeadGL.ParentId = Head5.Id
                    JOIN Head4 ON Head5.ParentId = Head4.Id
                    JOIN Head3 ON Head4.ParentId = Head3.Id
                    JOIN Head2 ON Head3.ParentId = Head2.Id
                   WHERE Head2.AccCode = '24' 
				         AND Voucher.CompanyId = @CompanyId
                         AND VoucherDetail.IsActive = 1
						 AND Voucher.IsActive = 1
                         AND Voucher.IsSubmit =1
                         AND Voucher.VoucherDate &lt;= @FromDate
						-- AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
                   GROUP BY Head3.AccName, Head3.Id, Head3.OrderNo) AS A
   WHERE A.Balance &lt;&gt; 0
   ORDER BY A.OrderNo</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="AccName"><DataField>AccName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="OrderNo"><DataField>OrderNo</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="ProfitLoss"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter><QueryParameter Name="@CostCenterId"><Value>=Parameters!CostCenterId.Value</Value></QueryParameter></QueryParameters><CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	SELECT
	(SELECT 
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END) as Balance 
	FROM VoucherDetail
     JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
     JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
     JOIN Head5 on HeadGL.ParentId = Head5.Id
     JOIN Head4 on Head5.ParentId = Head4.Id
     JOIN Head3 on Head4.ParentId = Head3.Id
     JOIN Head2 on Head3.ParentId = Head2.Id
     JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '3'
 and Head2.CompanyId =  @CompanyId 
 AND Voucher.IsActive =1
 AND VoucherDetail.IsActive =1
 AND Voucher.IsSubmit = 1
 and Voucher.VoucherDate  &lt;=  @FromDate
 AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
 Group By Head1.DrIncrease)
 -
 (SELECT 
		(CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END) as Balance 
	FROM VoucherDetail
     JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
     JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
     JOIN Head5 on HeadGL.ParentId = Head5.Id
     JOIN Head4 on Head5.ParentId = Head4.Id
     JOIN Head3 on Head4.ParentId = Head3.Id
     JOIN Head2 on Head3.ParentId = Head2.Id
     JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '4'
 AND Voucher.IsActive = 1
 AND VoucherDetail.IsActive = 1
  AND Voucher.IsSubmit = 1
 and Voucher.CompanyId = @CompanyId 
 and Voucher.VoucherDate  &lt;=  @FromDate 
 Group By Head1.DrIncrease) as Balance </CommandText></Query><Fields><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field></Fields></DataSet><DataSet Name="SignatoryDataSet"><Query><DataSourceName>BalanceSheetDS</DataSourceName><QueryParameters><QueryParameter Name="@StrFromDate"><Value>=Parameters!StrFromDate.Value</Value></QueryParameter><QueryParameter Name="@CompanyId"><Value>=Parameters!CompanyId.Value</Value></QueryParameter><QueryParameter Name="@ReportCategoryId"><Value>=Parameters!ReportCategoryId.Value</Value></QueryParameter></QueryParameters><CommandType>StoredProcedure</CommandType><CommandText>AccountingApprovalSignatory</CommandText></Query><Fields><Field Name="ApprovalStatus"><DataField>ApprovalStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ApprovedDate"><DataField>ApprovedDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="SignatoryName"><DataField>SignatoryName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FinalStatus"><DataField>FinalStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="SignatoryType"><DataField>SignatoryType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OrderBy"><DataField>OrderBy</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ReportStatus"><DataField>ReportStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>4.10499in</Width></TablixColumn><TablixColumn><Width>2.10499in</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.21667in</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox2"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>PROPERTIES &amp; ASSETS </Value><Style><FontF