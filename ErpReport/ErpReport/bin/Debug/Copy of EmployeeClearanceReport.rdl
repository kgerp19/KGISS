<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><am:AuthoringMetadata xmlns:am="http://schemas.microsoft.com/sqlserver/reporting/authoringmetadata"><am:CreatedBy><am:Name>SSDTRS</am:Name><am:Version>17.6.33723.286</am:Version></am:CreatedBy><am:UpdatedBy><am:Name>SSDTRS</am:Name><am:Version>17.6.33723.286</am:Version></am:UpdatedBy><am:LastModifiedTimestamp>2023-11-09T04:56:23.9859189Z</am:LastModifiedTimestamp></am:AuthoringMetadata><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="GeneralLedgerDS"><Transaction>true</Transaction><DataSourceReference>ErpDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>cdcb2f2c-7ca3-4782-8265-294164a2cd50</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="ExitInterviewInfo"><Query><DataSourceName>GeneralLedgerDS</DataSourceName><QueryParameters><QueryParameter Name="@id"><Value>=Parameters!id.Value</Value></QueryParameter></QueryParameters><CommandText>select e.Id as Id, e.EmployeeId,emp.Name,emp.JoiningDate, e.ExpectedResignDate, e.ReasonForLeaving,d.Name as DepartmentName, ds.Name as DesignationName from ExitInterview e
join Employee emp on e.EmployeeId = emp.EmployeeId 
join Department d on emp.DepartmentId = d.DepartmentId
join Designation ds on emp.DesignationId = ds.DesignationId
where e.Id = @id</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="JoiningDate"><DataField>JoiningDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="ExpectedResignDate"><DataField>ExpectedResignDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="ReasonForLeaving"><DataField>ReasonForLeaving</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DepartmentName"><DataField>DepartmentName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DesignationName"><DataField>DesignationName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="SignatoryStatusInfo"><Query><DataSourceName>GeneralLedgerDS</DataSourceName><QueryParameters><QueryParameter Name="@id"><Value>=Parameters!id.Value</Value></QueryParameter></QueryParameters><CommandText>select e.Id as Id, emp.EmployeeId,emp.Name,d.Name as DepartmentName, ds.Name as DesignationName , cm.Status, cm.Note as Comments
from ClearanceSignatoryMap cm
join ClearanceSignatory cs on cm.ClearanceSignatoryId = cs.ClearanceSignatoryId
join ExitInterview e on cm.ExitInterviewId = e.Id
join Employee emp on cs.EmployeeId = emp.Id 
join Department d on emp.DepartmentId = d.DepartmentId
join Designation ds on emp.DesignationId = ds.DesignationId
where e.Id = @id
order by cs.CompanyId,cs.DepartmentId
</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DepartmentName"><DataField>DepartmentName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DesignationName"><DataField>DesignationName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Status"><DataField>Status</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Comments"><DataField>Comments</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><Height>2.53194in</Height><Style><Border><Style>None</Style></Border></Style></Body><Width>8.69529in</Width><Page><PageHeader><Height>1.24181in</Height><PrintOnFirstPage>true</PrintOnFirstPage><Style><Border><Style>None</Style></Border></Style></PageHeader><PageHeight>12in</PageHeight><PageWidth>9in</PageWidth><LeftMargin>0.25in</LeftMargin><RightMargin>0.25in</RightMargin><TopMargin>0.25in</TopMargin><BottomMargin>0.25in</BottomMargin><Style><Border><Color>LightGrey</Color><Style>None</Style></Border></Style></Page></ReportSection></ReportSections><ReportParameters><ReportParameter Name="id"><DataType>String</DataType><Prompt>id</Prompt></ReportParameter></ReportParameters><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType>Inch</rd:ReportUnitType><rd:ReportID>53bd22cf-1c6b-4e62-9ea6-c504ecc0adea</rd:ReportID></Report>