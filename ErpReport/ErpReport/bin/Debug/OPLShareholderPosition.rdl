><FontSize>12pt</FontSize><FontWeight>Normal</FontWeight></Style></TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph><Paragraph><TextRuns><TextRun><Value>Share Holder's Position</Value><Style><FontFamily>Times New Roman</FontFamily><FontSize>14pt</FontSize><FontWeight>Bold</FontWeight><TextDecoration>Underline</TextDecoration></Style></TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName>Textbox27</rd:DefaultName><Left>0.025in</Left><Height>0.87722in</Height><Width>7.67501in</Width><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Image Name="CompanyLogo"><Source>External</Source><Value>=First(Fields!CompanyLogo.Value, "CompanyDSet")</Value><Sizing>FitProportional</Sizing><Top>0.00499in</Top><Left>1.025in</Left><Height>0.83403in</Height><Width>0.9875in</Width><ZIndex>1</ZIndex><ToolTip>=First(Fields!CompanyLogo.Value, "CompanyDSet")</ToolTip><Style><Border><Style>None</Style></Border></Style></Image></ReportItems><Style><Border><Style>None</Style></Border></Style></PageHeader><PageHeight>11.69in</PageHeight><PageWidth>8.27in</PageWidth><LeftMargin>0.25in</LeftMargin><RightMargin>0.25in</RightMargin><TopMargin>0.25in</TopMargin><BottomMargin>0.25in</BottomMargin><Style /></Page></ReportSection></ReportSections><ReportParameters><ReportParameter Name="StrFromDate"><DataType>String</DataType><Prompt>From Date</Prompt></ReportParameter><ReportParameter Name="CompanyId"><DataType>Integer</DataType><Prompt>Company</Prompt></ReportParameter></ReportParameters><Code>Public Function SetBDCurrency(ByVal a As String) As String
    Dim info = New System.Globalization.CultureInfo("hi-IN")
    Dim valueApplied As String
    Dim value As Decimal = Decimal.Round(Convert.ToDecimal(a), 2)

    If value &lt; 0D Then
        Dim minusStr As String = value.ToString()
        Dim subtracSign As String = minusStr.Substring(1, minusStr.Length - 1)
        Dim minusValue As Decimal = Math.Round(Convert.ToDecimal(subtracSign), 2)

        If minusValue &lt; 1000000000 Then
            valueApplied = "(" &amp; minusValue.ToString("N2", info) &amp; ")"
        Else
            Dim decimalValue As Decimal = Decimal.Round(Convert.ToDecimal(minusValue), 2)
            Dim strValue As String = decimalValue.ToString()

            If Not strValue.Contains(".") Then
                strValue = strValue &amp; ".00"
            End If

            Dim formatingPart As String
            formatingPart = strValue.Substring((strValue.Length - 1) - 10, 11)
            Dim addOneformatingPart As String = "1" &amp; formatingPart
            Dim firstPart As String

            If minusValue &lt; 10000000000 Then
                firstPart = strValue.Substring(0, 2)
            ElseIf minusValue &lt; 100000000000 Then
                firstPart = strValue.Substring(0, 3)
            ElseIf minusValue &lt; 1000000000000 Then
                firstPart = strValue.Substring(0, 4)
            ElseIf minusValue &lt; 10000000000000 Then
                firstPart = strValue.Substring(0, 5)
            ElseIf minusValue &lt; 100000000000000 Then
                firstPart = strValue.Substring(0, 6)
            Else
                firstPart = strValue.Substring(0, 7)
            End If

            Dim formatingPartvalue As Decimal = Decimal.Round(Convert.ToDecimal(addOneformatingPart), 2)
            Dim formatedval As String = formatingPartvalue.ToString("N2", info)
            Dim subtracOneformatingPart As String = formatedval.Substring(1, formatedval.Length - 1)
            valueApplied = firstPart &amp; subtracOneformatingPart
        End If
    Else

        If value &lt; 1000000000 Then
            valueApplied = value.ToString("N2", info)
        Else
            Dim decimalValue As Decimal = Decimal.Round(Convert.ToDecimal(value), 2)
            Dim strValue As String = decimalValue.ToString()

            If Not strValue.Contains(".") Then
                strValue = strValue &amp; ".00"
            End If

            Dim formatingPart As String
            formatingPart = strValue.Substring((strValue.Length - 1) - 10, 11)
            Dim addOneformatingPart As String = "1" &amp; formatingPart
            Dim firstPart As String

            If value &lt; 10000000000 Then
                firstPart = strValue.Substring(0, 2)
            ElseIf value &lt; 100000000000 Then
                firstPart = strValue.Substring(0, 3)
            ElseIf value &lt; 1000000000000 Then
                firstPart = strValue.Substring(0, 4)
            ElseIf value &lt; 10000000000000 Then
                firstPart = strValue.Substring(0, 5)
            ElseIf value &lt; 100000000000000 Then
                firstPart = strValue.Substring(0, 6)
            Else
                firstPart = strValue.Substring(0, 7)
            End If

            Dim formatingPartvalue As Decimal = Decimal.Round(Convert.ToDecimal(addOneformatingPart), 2)
            Dim formatedval As String = formatingPartvalue.ToString("N2", info)
            Dim subtracOneformatingPart As String = formatedval.Substring(1, formatedval.Length - 1)
            valueApplied = firstPart &amp; subtracOneformatingPart
        End If
    End If

    Return valueApplied
End Function



