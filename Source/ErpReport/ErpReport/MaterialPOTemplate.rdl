    <DataSet Name="PurchaseOrderItemDSet">
      <Query>
        <DataSourceName>PurchaseOrderTemplateDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PurchaseOrderId">
            <Value>=Parameters!PurchaseOrderId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
      POD.PurchaseOrderDetailId, P.ProductName,COALESCE(C.CountryName,PO.CompanyName,'Local') as CountryName,
	  POD.PurchaseRate,CAST(POD.PurchaseQty as DECIMAL(18)) as PurchaseQty,POD.PackSize,PO.DeliveryDate
from Erp.PurchaseOrderDetail POD
inner join Erp.Product P on POD.ProductId=P.ProductId
inner join Erp.PurchaseOrder PO on POD.PurchaseOrderId=PO.PurchaseOrderId
left join Country C on PO.CountryId=C.CountryId
where PO.PurchaseOrderId=@PurchaseOrderId</CommandText>
      </Query>
