    <DataSet Name="CompanyDSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Name,
                             (SELECT        Url
                               FROM            UrlInfo
                               WHERE        (UrlType = 1)) + 'Images/Logo/' + CompanyLogo AS CompanyLogo, Address, Phone
FROM            Company
WHERE        (CompanyId = @CompanyId)</CommandText>
      </Query>
