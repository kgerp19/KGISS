  <DataSets>
    <DataSet Name="ExitInterviewInfo">
      <Query>
        <DataSourceName>GeneralLedgerDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select e.Id as Id, e.EmployeeId,emp.Name,emp.JoiningDate, e.ExpectedResignDate, e.ReasonForLeaving,c.Name as CompanyName,d.Name as DepartmentName, ds.Name as DesignationName ,
CASE 
        WHEN e.Status = 0 THEN 'Draft'
        WHEN e.Status = 1 THEN 'Submitted'
        WHEN e.Status = 2 THEN 'Pending'
        WHEN e.Status = 3 THEN 'Approved'
        WHEN e.Status = 4 THEN 'Rejected'
        ELSE 'Not Ok' 
    END AS FeedbackStatus
from ExitInterview e
join Employee emp on e.EmployeeId = emp.EmployeeId 
join Company c on emp.CompanyId = c.CompanyId
join Department d on emp.DepartmentId = d.DepartmentId
join Designation ds on emp.DesignationId = ds.DesignationId
where e.Id = @id</CommandText>
      </Query>
