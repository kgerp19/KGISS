    <DataSet Name="SignatoryStatusInfo">
      <Query>
        <DataSourceName>GeneralLedgerDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select e.Id as Id, emp.EmployeeId,emp.Name,d.Name as DepartmentName,c.Name as CompanyName, ds.Name as DesignationName , cm.Note as Comments,
CASE      
        WHEN cm.Status = 0 THEN 'Pending'
        WHEN cm.Status = 1 THEN 'Approved'
        WHEN cm.Status = 2 THEN 'Rejected'
        ELSE 'Not Ok' 
    END AS SignatoryStatus

from ClearanceSignatoryMap cm
join ClearanceSignatory cs on cm.ClearanceSignatoryId = cs.ClearanceSignatoryId
join ExitInterview e on cm.ExitInterviewId = e.Id
join Employee emp on cs.EmployeeId = emp.Id 
join Company c on emp.CompanyId = c.CompanyId
join Department d on emp.DepartmentId = d.DepartmentId
join Designation ds on emp.DesignationId = ds.DesignationId
where e.Id = @id
order by cs.CompanyId,cs.DepartmentId</CommandText>
      </Query>
