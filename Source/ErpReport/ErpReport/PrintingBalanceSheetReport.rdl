  <DataSets>
    <DataSet Name="CompanyDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
 DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

SELECT     @FromDate as FromDate,   Name, Address,
                             (SELECT        Url
                               FROM            UrlInfo
                               WHERE        (UrlType = 1)) + 'Images/Logo/' + CompanyLogo AS CompanyLogo
FROM            Company
WHERE        (CompanyId = @CompanyId)</CommandText>
      </Query>
      <Fields>
        <Field Name="FromDate">
          <DataField>FromDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Address">
          <DataField>Address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyLogo">
          <DataField>CompanyLogo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AssetDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

	SELECT * FROM (SELECT  Head2.AccName as AccName, 
						   IsNull(SUM(IsNull(VoucherDetail.DebitAmount,0)) - SUM(IsNull(VoucherDetail.CreditAmount,0)),0) as Balance, 
						  Head2.AccCode as AccCode 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                    WHERE Head1.AccCode = 1 
					And Head2.AccCode &lt;&gt; '13' 
					and Voucher.CompanyId = @CompanyId
					AND Voucher.IsActive = 1
                    and VoucherDetail.IsActive = 1
                    AND Voucher.IsSubmit = 1
                    and Voucher.VoucherDate  &lt;=  @FromDate 
                    GROUP BY Head2.AccName, 
					         Head2.Id, 
							 Head2.AccCode) AS A
   WHERE A.Balance &lt;&gt; 0
   order by A.AccCode</CommandText>
      </Query>
      <Fields>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AccCode">
          <DataField>AccCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CurrentAssetDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))

	SELECT * 
	FROM (
	SELECT       Head4.AccName AS AccName, 
				 IsNull(SUM(IsNull(VoucherDetail.DebitAmount,0)) - SUM(IsNull(VoucherDetail.CreditAmount,0)),0) AS Balance, 
				 Head4.AccCode AS AccCode 
	      FROM VoucherDetail
           JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
           JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
           JOIN Head5 ON HeadGL.ParentId = Head5.Id
           JOIN Head4 ON Head5.ParentId = Head4.Id
           JOIN Head3 ON Head4.ParentId = Head3.Id
           JOIN Head2 ON Head3.ParentId = Head2.Id
          WHERE Head2.AccCode = '13' 
		        AND Voucher.CompanyId = @CompanyId 
				AND Voucher.IsActive = 1
				AND VoucherDetail.IsActive = 1
                AND Voucher.IsSubmit= 1
                AND Voucher.VoucherDate  &lt;= @FromDate
				AND Head3.AccCode &lt;&gt; '1305'				
          GROUP BY Head4.AccName, Head4.AccCode
		  UNION ALL

		  SELECT Head4.AccName AS AccName, 
				 IsNull(SUM(IsNull(VoucherDetail.DebitAmount,0)) - SUM(IsNull(VoucherDetail.CreditAmount,0)),0) AS Balance, 
				 Head4.AccCode AS AccCode 
	      FROM VoucherDetail
           JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
           JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
           JOIN Head5 ON HeadGL.ParentId = Head5.Id
           JOIN Head4 ON Head5.ParentId = Head4.Id
           JOIN Head3 ON Head4.ParentId = Head3.Id
           JOIN Head2 ON Head3.ParentId = Head2.Id
          WHERE  Head3.AccCode = '1305'
		        AND Voucher.CompanyId =  @CompanyId 
				AND Voucher.IsActive = 1
				AND VoucherDetail.IsActive = 1
                 AND Voucher.IsSubmit= 1
                AND Voucher.VoucherDate  &lt;=  @FromDate
			 			
          GROUP BY Head4.AccName, Head4.AccCode
		  
		  ) AS A
   WHERE A.Balance &lt;&gt; 0 
   ORDER BY A.AccCode	</CommandText>
      </Query>
      <Fields>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AccCode">
          <DataField>AccCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CapitalLiabilityDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	SELECT * FROM (
	
	SELECT 
	                      Head2.AccName as AccName, 
						 IsNull(SUM(Isnull(CreditAmount,0)) - SUM(Isnull(DebitAmount,0)),0) as Balance, 
						  Head2.AccCode as AccCode 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                   WHERE Head1.AccCode = '2'
				         AND Head2.AccCode &lt;&gt; '24'
						 AND Head2.AccCode &lt;&gt; '21'
						 AND Voucher.CompanyId =  @CompanyId
						 AND Voucher.IsActive = 1
						 AND VoucherDetail.IsActive =1
                         AND Voucher.IsSubmit = 1
                         AND Voucher.VoucherDate &lt;= @FromDate
						
                   GROUP BY Head2.AccName, Head2.AccCode
				   

				   Union All

				   SELECT Head3.AccName as AccName, 
						  IsNull(SUM(Isnull(CreditAmount,0)) - SUM(Isnull(DebitAmount,0)),0) as Balance, 
						  Head3.AccCode as AccCode 
	               FROM VoucherDetail
                    JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 on HeadGL.ParentId = Head5.Id
                    JOIN Head4 on Head5.ParentId = Head4.Id
                    JOIN Head3 on Head4.ParentId = Head3.Id
                    JOIN Head2 on Head3.ParentId = Head2.Id
                    JOIN Head1 on Head2.ParentId = Head1.Id

                   WHERE Voucher.IsActive = 1 
				         AND VoucherDetail.IsActive = 1
						 AND Voucher.IsSubmit = 1
						 AND Head2.AccCode = '21'
						 AND Voucher.CompanyId =  @CompanyId
                         AND Voucher.VoucherDate &lt;=  @FromDate
						
                   GROUP BY Head3.AccName, Head3.AccCode

				   
				   
				   
				   ) AS A
   WHERE Round(A.Balance,0) &lt;&gt; 0
   ORDER BY A.AccCode</CommandText>
      </Query>
      <Fields>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AccCode">
          <DataField>AccCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CurrentLiabilitiesDSet">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105)) 

	SELECT * FROM (SELECT Head3.Id, 
	                      Head3.AccName AS AccName, 
						  (SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount)) AS Balance, 
						  Head3.AccCode AS AccCode 
	               FROM VoucherDetail
                    JOIN Voucher ON VoucherDetail.VoucherId = Voucher.VoucherId
                    JOIN HeadGL ON VoucherDetail.AccountHeadId = HeadGL.Id
                    JOIN Head5 ON HeadGL.ParentId = Head5.Id
                    JOIN Head4 ON Head5.ParentId = Head4.Id
                    JOIN Head3 ON Head4.ParentId = Head3.Id
                    JOIN Head2 ON Head3.ParentId = Head2.Id
                   WHERE Head2.AccCode = '24' 
				         AND Voucher.CompanyId = @CompanyId
                         AND VoucherDetail.IsActive = 1
						 AND Voucher.IsActive = 1
                         AND Voucher.IsSubmit = 1
                         AND Voucher.VoucherDate &lt;= @FromDate						
                   GROUP BY Head3.AccName, Head3.Id, Head3.AccCode) AS A
   WHERE Round(A.Balance,2) &lt;&gt; 0
   ORDER BY A.AccCode</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AccCode">
          <DataField>AccCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ProfitLoss">
      <Query>
        <DataSourceName>BalanceSheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CostCenterId">
            <Value>=Parameters!CostCenterId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @FromDate DATE
    SELECT @FromDate = CONVERT(VARCHAR, CONVERT(DATE, @StrFromDate, 105))   

	SELECT
	Round(IsNull((SELECT 
		IsNull((CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END),0) as Balance 
	FROM VoucherDetail
     JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
     JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
     JOIN Head5 on HeadGL.ParentId = Head5.Id
     JOIN Head4 on Head5.ParentId = Head4.Id
     JOIN Head3 on Head4.ParentId = Head3.Id
     JOIN Head2 on Head3.ParentId = Head2.Id
     JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '3'
 and Voucher.CompanyId =  @CompanyId 
 AND Voucher.IsActive = 1 
 AND VoucherDetail.IsActive = 1
 AND Voucher.IsSubmit = 1
 and Voucher.VoucherDate  &lt;=  @FromDate
 AND (@CostCenterId = 0  or Voucher.Accounting_CostCenterFk = @CostCenterId)
 Group By Head1.DrIncrease),0)
 -
 IsNull((SELECT 
		IsNull((CASE
		WHEN Head1.DrIncrease = 1
			THEN IsNull(SUM(VoucherDetail.CreditAmount) - SUM(VoucherDetail.DebitAmount),0)
			ELSE  IsNull(SUM(VoucherDetail.DebitAmount) - SUM(VoucherDetail.CreditAmount),0)
	END),0) as Balance 
	FROM VoucherDetail
     JOIN Voucher on VoucherDetail.VoucherId = Voucher.VoucherId
     JOIN HeadGL on VoucherDetail.AccountHeadId = HeadGL.Id
     JOIN Head5 on HeadGL.ParentId = Head5.Id
     JOIN Head4 on Head5.ParentId = Head4.Id
     JOIN Head3 on Head4.ParentId = Head3.Id
     JOIN Head2 on Head3.ParentId = Head2.Id
     JOIN Head1 on Head2.ParentId = Head1.Id

 where Head1.AccCode = '4'
 and Voucher.CompanyId = @CompanyId 
 AND Voucher.IsActive = 1 AND VoucherDetail.IsActive = 1
 AND Voucher.IsSubmit = 1
 and Voucher.VoucherDate  &lt;=  @FromDate 
 Group By Head1.DrIncrease),0),2) as Balance </CommandText>
      </Query>
      <Fields>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
