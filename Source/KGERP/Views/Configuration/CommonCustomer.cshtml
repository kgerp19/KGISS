
@{
    ViewData["Title"] = "Dealer / Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
@model KGERP.Service.Configuration.VMCommonSupplier

<style type="text/css">

    input, select, textarea {
        max-width: 100%;
    }


    .form-floating > label {
        font-size: 1rem;
    }

    .card {
        border-radius: 10px;
        border: 1px solid #E0E0E0;
    }

    fieldset {
        border: 2px solid #DEE2E6;
    }

    legend {
        width: auto;
        padding: 0 10px;
    }

    .btn-lg {
        padding: 0.75rem 1.5rem;
    }

</style>

<section class="content-header">
    <h1>
        Dealer / Customer
    </h1>
    <ol class="breadcrumb">
        <li><a href="~/Home/Index"><i class="fa fa-dashboard"></i>Dashboard</a></li>
        <li class="active">Dealer / Customer</li>
    </ol>
    <br />
    <div class="row">
        <div class="col-md-12">
            @using (Html.BeginForm("CommonCustomer", "Configuration", FormMethod.Post, new { id = "commonCustomerformId", @class = "formValidator", @enctype = "multipart/form-data" }))
            {
                <div class="">
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    <div class="box-danger">

                        <!-- /.box-header -->
                        <div class="">
                            <div class="card shadow-sm p-4 border-0 mb-4">
                                <!-- Hidden Inputs -->
                                @Html.HiddenFor(model => model.ID, new { id = "customerId" })
                                @Html.HiddenFor(model => model.ActionId, new { id = "ActionId" })
                                @Html.HiddenFor(model => model.CompanyFK, new { id = "CompanyFK" })

                                <!-- Basic Information Section -->
                                <fieldset class="border p-3 rounded " style="padding:10px;">
                                    <legend class="text-primary font-weight-bold">Basic Information</legend>
                                    <div class="row g-4 ">
                                        <!-- Dealer/Customer Name -->
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtCustomerName">Dealer/Customer Name</label>
                                                @Html.TextBoxFor(model => model.Name, new { id = "txtCustomerName", @class = "form-control", placeholder = "Customer Name", required = "required" })

                                                @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <!-- Phone -->
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtPhone">Phone</label>
                                                @Html.TextBoxFor(model => model.Phone, new { id = "txtPhone", @class = "form-control", placeholder = "Contact" })

                                                @Html.ValidationMessageFor(model => model.Phone, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <!-- Email -->
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtEmail">Email</label>
                                                @Html.TextBoxFor(model => model.Email, new { id = "txtEmail", @class = "form-control", placeholder = "Email" })

                                                @Html.ValidationMessageFor(model => model.Email, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <!-- Payment Type -->
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtPaymentType">Payment Type</label>
                                                @Html.DropDownListFor(model => model.PaymentType, Model.PaymentTypeList as SelectList, "--Payment Type--", new { id = "txtPaymentType", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.PaymentType, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row g-4 mt-3">
                                        <!-- Division, District, Upazila -->
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="ddlCommon_DivisionFk">Division</label>
                                                @Html.DropDownListFor(model => model.Common_DivisionFk, Model.DivisionList as SelectList, "--Division--", new { id = "ddlCommon_DivisionFk", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.Common_DivisionFk, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="ddlCommon_DistrictsFk">District</label>
                                                @Html.DropDownListFor(model => model.Common_DistrictsFk, Model.DistrictList as SelectList, "--District--", new { id = "ddlCommon_DistrictsFk", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.Common_DistrictsFk, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="ddlCommon_UpazilasFk">Upazila</label>
                                                @Html.DropDownListFor(model => model.Common_UpazilasFk, Model.UpazilasList as SelectList, "--Upazila--", new { id = "ddlCommon_UpazilasFk", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.Common_UpazilasFk, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <!-- Zone and Territory -->
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="ddlZoneId">Zone</label>
                                                @Html.DropDownListFor(model => model.ZoneId, Model.ZoneListList as SelectList, "--Select Zone--", new { id = "ddlZoneId", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.ZoneId, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                    </div>

                                    <!-- Additional Information -->
                                    <div class="row g-4 mt-3">
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="ddlSubZoneId">Territory</label>
                                                @Html.DropDownListFor(model => model.SubZoneId, Model.TerritoryList as SelectList, "--Select Territory--", new { id = "ddlSubZoneId", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.SubZoneId, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtContactPerson">Contact Person</label>
                                                @Html.TextBoxFor(model => model.ContactPerson, new { id = "txtContactPerson", @class = "form-control", placeholder = "Contact Person" })

                                                @Html.ValidationMessageFor(model => model.ContactPerson, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtSecurityAmount">Security Amount</label>
                                                @Html.TextBoxFor(model => model.SecurityAmount, new { id = "txtSecurityAmount", @class = "form-control", placeholder = "Security Amount" })

                                                @Html.ValidationMessageFor(model => model.SecurityAmount, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="ddlCustomerStatus">Customer Status</label>
                                                @Html.DropDownListFor(model => model.CustomerStatus, Model.CustomerStatusEnumList as SelectList, "--Select Customer Status--", new { id = "ddlCustomerStatus", @class = "form-control", required = "true" })

                                                @Html.ValidationMessageFor(model => model.CustomerStatus, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                    </div>



                                    <div class="row g-4 mt-3">
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtPropietor">Propietor</label>
                                                @Html.TextBoxFor(model => model.Propietor, new { id = "txtPropietor", @class = "form-control", placeholder = "Propietor" })

                                                @Html.ValidationMessageFor(model => model.Propietor, "", new { @class = "text-danger" })
                                            </div>
                                        </div>


                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtPropietor">Credit Limit</label>
                                                @Html.TextBoxFor(model => model.CreditLimit, new { id = "txtPropietor", @class = "form-control", placeholder = "Credit Limit" })

                                                @Html.ValidationMessageFor(model => model.CreditLimit, "", new { @class = "text-danger" })
                                            </div>
                                        </div>


                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label for="message-text" class="col-form-label">Customer Type:</label>
                                                @Html.DropDownListFor(model => model.CustomerTypeFk, Model.CustomerTypeList as SelectList, "--Customer Type--", new { id = "ddCustomerTypeFk", @class = "form-control", required = "true" })
                                                @Html.ValidationMessageFor(model => model.CustomerTypeFk, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtNid">NID</label>
                                                @Html.TextBoxFor(model => model.NID, new { id = "txtNid", @class = "form-control", placeholder = "NID" })

                                                @Html.ValidationMessageFor(model => model.NID, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row g-4 mt-3">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label for="message-text" class="col-form-label">Address:</label>
                                                @Html.TextAreaFor(model => model.Address, new { id = "txtAddress", @class = "form-control", @placeholder = "Address" })
                                                @*@Html.ValidationMessageFor(model => model.Address, "", new { @class = "text-danger" })*@
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">Customer Image</label>
                                                <input type="file" name="Image" id="txtImageFile" class="form-control" accept="image/*" />
                                                <small class="form-text text-muted">Please upload an image file (JPG, PNG, GIF).</small>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">NID Card</label>
                                                <input type="file" name="NidImage" id="txtImageFile" class="form-control" accept="image/*" />
                                                <small class="form-text text-muted">Please upload an image file (JPG, PNG, GIF).</small>
                                            </div>
                                        </div>







                                    </div>
                                </fieldset>

                                <br />
                                <!-- Bank Account Information Section -->
                                <fieldset class="border p-3 rounded mt-4" style="padding:10px;">
                                    <legend class="text-primary font-weight-bold">Bank Account Information</legend>
                                    <div class="row g-4">
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label >Account Holder Name</label>
                                                @Html.TextBoxFor(model => model.ACName, new { id = "txtACName", @class = "form-control", placeholder = "Account Holder Name", required = "required" })

                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label >Bank Name</label>
                                                @Html.TextBoxFor(model => model.BankName, new { id = "txtCustomerBankAc", @class = "form-control", placeholder = "Bank Name", required = "required" })
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label >Branch Name</label>
                                                @Html.TextBoxFor(model => model.BranchName, new { id = "txtCustomerBanBrancekAc", @class = "form-control", placeholder = "Branch Name", required = "required" })

                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label >Account Number</label>
                                                @Html.TextBoxFor(model => model.ACNo, new { id = "txtCustomerBankAcNo", @class = "form-control", placeholder = "Account Number", required = "required" })

                                            </div>
                                        </div>
                                    </div>
                                    <div class="row g-4">

                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label>Cheque Details</label>
                                                @Html.DropDownListFor(model => model.CheckDetailId, Model.Checkdetail as SelectList, "--Cheque Details--", new { id = "ddChequeDetailsFk", @class = "form-control"})

                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label >Cheque Number</label>
                                                @Html.TextBoxFor(model => model.CheckNo, new { id = "txtCustomerBankAcCkNo", @class = "form-control", placeholder = "Cheque Number" })

                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label >Cheque type</label>
                                                @Html.DropDownListFor(model => model.CheckTypeId, Model.CheckType as SelectList, "--Cheque type--", new { id = "ddCustomerCheckTypeFk", @class = "form-control"})

                                            </div>
                                        </div>
                                    </div>
                                </fieldset>


                                <br />
                                <!-- Bank Account Information Section -->
                                <fieldset class="border p-3 rounded mt-4" style="padding:10px;">
                                    <legend class="text-primary font-weight-bold">Business Document</legend>
                                    <small class="form-text text-muted">Please upload an image file (JPG, PNG, GIF).</small>
                                    <div class="row g-4">
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label  for="txtImageFile" class="form-label">Trade Licence</label>
                                                <input type="file" name="TradeLicenceUrl" id="txtImageFile" class="form-control" accept="image/*"  />


                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">BSA Membership</label>
                                                <input type="file" name="BSAMemUrl" id="txtImageFile" class="form-control" accept="image/*"  />


                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">Whole Sale Licence</label>
                                                <input type="file" name="SaleLiUrl"  id="txtImageFile" class="form-control" accept="image/*"  />


                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">Seed Dealer Licence</label>
                                                <input type="file" name="DelerLiUrl" id="txtImageFile" class="form-control" accept="image/*"  />


                                            </div>
                                        </div>
                                    </div>
                                    <div class="row g-4">

                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">TIN No</label>
                                                <input type="file" name="TinUrl" id="txtImageFile" class="form-control" accept="image/*"  />

                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-floating">
                                                <label for="txtImageFile" class="form-label">Bank Cheque</label>
                                                <input type="file" name="BankChkUrl" id="txtImageFile" class="form-control" accept="image/*"  />


                                            </div>
                                        </div>
                                     
                                    </div>
                                </fieldset>

                                <!-- Submission Button -->
                                <br />
                                <div class="form-group">
                                    <div class="col-md-3">
                                        @*<button type="button" onclick="resetForm()" class="btn btn-secondary" data-dismiss="modal">Reset</button>*@
                                    </div>
                                    <div class="col-md-4">
                                        <button type="submit" id="btnSave" class="btn btn-primary btn-block"><i class='fa fa-plus'></i> Add</button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Custom Styles -->



                        <div class="box-footer">
                        </div>
                        <div class="box">
                            <div class="box-danger">
                                <div class="box-header">

                                </div>
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <div class="table-responsive text-nowrap">
                                        <table id="KgErpDataTable" class="table table-striped">
                                            <thead>
                                                <tr>
                                                    <th>SL</th>
                                                    <th>Code</th>
                                                    <th>

                                                        Name
                                                    </th>
                                                    <th>
                                                        Customer Type
                                                    </th>
                                                    <th>Payment Type</th>
                                                    <th>
                                                        Phone
                                                    </th>
                                                    <th>
                                                        Email
                                                    </th>
                                                    <th>
                                                        NID
                                                    </th>
                                                    <th>
                                                        Credit Limit
                                                    </th>
                                                    <th>
                                                        Address
                                                    </th>
                                                    <th>
                                                        Zone
                                                    </th>

                                                    <th>
                                                        Security Amount
                                                    </th>
                                                    <th>
                                                        Status
                                                    </th>
                                                    <th>
                                                        Propietor
                                                    </th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            @{int sl = 0;
                                            }
                                            <tbody>
                                                @foreach (var item in Model.DataList)
                                                {
                                                    <tr>
                                                        <td>@(++sl)</td>
                                                        <td>
                                                            @item.Code
                                                        </td>
                                                        <td>
                                                            <a href="~/Configuration/CommonCustomerByID?customerId=@item.ID" target="_blank">@item.Name</a>

                                                        </td>
                                                        <td>
                                                            <span class="bg-gray-light">
                                                                @item.CustomerType
                                                            </span>
                                                        </td>
                                                        <td>
                                                            @item.PaymentType
                                                        </td>
                                                        <td>
                                                            @item.Phone
                                                        </td>
                                                        <td>
                                                            @item.Email
                                                        </td>
                                                        <td>
                                                            @item.NID
                                                        </td>
                                                        <td>
                                                            @item.CreditLimit
                                                        </td>
                                                        <td>
                                                            @item.Address, @item.Upazila, @item.Division @item.District, @item.Country
                                                        </td>
                                                        <td>
                                                            @item.ZoneName (Incharge: @item.ZoneIncharge)
                                                        </td>
                                                        <td>
                                                            @item.SecurityAmount
                                                        </td>
                                                        <td>
                                                            @item.CustomerStatus
                                                        </td>
                                                        <td>
                                                            @item.Propietor
                                                        </td>
                                                        <td>
                                                            <a id="@item.ID" onclick="makeEditble(@item.ID);" class="linkPointer" title="Edit"><i class="fa fa-pencil-square-o"></i></a>
                                                            <a id="@item.ID" onclick="makeDeletable(@item.ID);" class="linkPointer" data-toggle="modal" data-target="#deleteModal" title="Delete"
                                                               data-placement="top">
                                                                <i class="glyphicon glyphicon-trash" aria-hidden="true"></i>
                                                            </a>

                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>




                                </div>
                                <div class="box-footer">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }


        </div>

    </div>
</section>


<section class="content">

</section>


@using (Html.BeginForm("CommonCustomer", "Configuration", FormMethod.Post, new { id = "commonCustomerformId", @class = "formValidator", @enctype = "multipart/form-data" }))
{
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    @Html.HiddenFor(model => model.ID, new { Value = "", id = "customerIdDelete" })
    @Html.HiddenFor(model => model.ActionId, new { Value = "", id = "ActionIdDelete" })
    @Html.HiddenFor(model => model.CompanyFK, new { id = "CompanyFKDelete" })

    <div class="modal fade" id="deleteModal" data-toggle="modal" data-backdrop="static" data-keyboard="false" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close pull-right" data-dismiss="modal" aria-label="Close">
                        &times;
                    </button>
                    <h5 class="modal-title"><b>Customer Delete</b></h5>

                </div>
                <div class="modal-body">
                    <div style="text-align:center">
                        <h3>Are you sure to delete this customer?</h3>
                    </div>
                </div>

                <div class="modal-footer">
                    <button type="button" id="btnClose" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" id="btnDelete" class="deleteId btn btn-primary"><i class="glyphicon glyphicon-check"></i>Confirm</button>
                </div>
            </div>
        </div>
    </div>
}


@section scripts {

    <script type="text/javascript">


        $(document).ready(function () {
            $('.formValidator').bootstrapValidator();


        });
        $("#ddlZoneId").change(function (e) {
            var zoneId = $("#ddlZoneId :selected").val();
                $.ajax({
                type: "GET",
                url: "@Url.Action("CommonSubZonesGet", "Configuration")",
                datatype: "Json",
                    data: { id: zoneId },
                    success: function (data) {
                    var items = "";
                        if (data) {
                            $("#ddlSubZoneId").empty();
                            $("#ddlSubZoneId").append("<option value=''>==Select Territory==</option>");
                            $(data).each(function (index, item) {
                                $("#ddlSubZoneId").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                            });

                        }
                    }
                });
       });

         $("#ddlCommon_DivisionFk").change(function (e) {
                var commonDivisionFk = $("#ddlCommon_DivisionFk :selected").val();
                $.ajax({
                type: "GET",
                url: "@Url.Action("CommonDistrictsGet","Configuration")",
                datatype: "Json",
                data: { id:  commonDivisionFk },
                    success: function (data) {
                    var items = "";
                        if (data) {
                            $("#ddlCommon_DistrictsFk").empty();
                            $("#ddlCommon_DistrictsFk").append("<option value=''>==Select District==</option>");
                            $(data).each(function (index, item) {
                                $("#ddlCommon_DistrictsFk").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                            });

                        }
                    }
                });
       });


        $("#ddlCommon_DistrictsFk").change(function (e) {
                var commonDistrictsFk = $("#ddlCommon_DistrictsFk :selected").val();
                $.ajax({
                type: "GET",
                url: "@Url.Action("CommonUpazilasGet", "Configuration")",
