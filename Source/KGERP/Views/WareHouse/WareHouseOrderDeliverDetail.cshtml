@model KGERP.Services.WareHouse.VMOrderDeliverDetail
@using KGERP.Utility;
@{
    ViewData["Title"] = "Delivery Challan";
    Layout = "~/Views/Shared/_Layout.cshtml";

    double TotalPrice = 0;
    decimal TotalDiscount = 0;
    decimal TotalAmount = 0;

    decimal TotalUnitDiscount = 0;
    double TotalGrossAmount = 0;
    
    decimal? TotalSpetialDiscount = 0;

}

<style type="text/css">

    input, select, textarea {
        max-width: 100%;
    }
</style>


<section class="content-header">
    <h1>
        @if (Model.OrderDeliverId > 0)
        {
            <span>
                Delivery Challan   <a href="@Url.Action("GetDeliveryChallanReport","Report", new {orderDeliverId=Model.OrderDeliverId,companyId=Model.CompanyFK})" title="Challan Copy" target="_blank"> @Model.ChallanNo </a>
            </span>
        }
        else
        {
            <span>
                Delivery Challan
            </span>
        }


    </h1>
    <ol class="breadcrumb">
        <li><a href="~/Home/Index"><i class="fa fa-dashboard"></i>Dashboard</a></li>
        <li class="active">Delivery Challan Information</li>
        <li class="active">
            <a class="btn-link btn btn-primary btn-xs" target="_blank" href="~/WareHouse/WareHouseOrderDeliverDetail?companyId=@Model.CompanyFK">Add New Deliver</a>

        </li>
    </ol>
</section>
@using (Html.BeginForm("WareHouseOrderDeliverDetail", "WareHouse", FormMethod.Post, new { id = "wareHouseOrderDeliverDetailFromID", @class = "formValidator" }))
{
    <section class="content">
        <div class="row">
            <div class="box">
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(x => x.CompanyFK, new { id = "CompanyFK" })

                <div class="box-danger">
                    <div class="box-header">

                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        @if (Model.OrderDeliverId > 0)
                        {
                            @Html.HiddenFor(x => x.CompanyFK, new { id = "CompanyFK" })


                            <div class="col-md-1"> </div>
                            <div class="col-md-10">
                                <table class="table">

                                    <tr>
                                        <td width="15%"><b>Challan No</b></td>
                                        <td width="1%">:</td>
                                        <th width="34%">@Model.ChallanNo</th>

                                        <td width="15%"><b>Delivery Date</b></td>
                                        <td width="1%">:</td>
                                        <td width="34%">@Model.DeliveryDate.Value.ToString("dd-MM-yyyy")</td>
                                    </tr>
                                    <tr>
                                        <th>
                                            Order No
                                        </th>
                                        <th>:</th>
                                        <td>
                                            <label class="col-form-label">@Model.OrderNo</label>
                                        </td>
                                        <th> Date</th>
                                        <th>:</th>
                                        <td>@Model.OrderDate.ToShortDateString()</td>
                                    </tr>
                                    <tr>
                                        <th>
                                            Invoice No
                                        </th>
                                        <th>:</th>
                                        <td>
                                            <label class="col-form-label">@Model.InvoiceNo</label>
                                        </td>
                                        <th>Invoice Date</th>
                                        <th>:</th>
                                        <td>@Model.DeliveryDate.Value.ToShortDateString()</td>
                                    </tr>

                                    <tr>
                                        <th>Zone</th>
                                        <th>:</th>
                                        <td>@Model.ZoneName</td>

                                        <th>Incharge</th>
                                        <th>:</th>
                                        <td>@Model.ZoneIncharge</td>
                                    </tr>
                                    <tr>
                                        <th>Customer</th>
                                        <th>:</th>
                                        <td>@Model.CustomerName</td>
                                        <th>Phone</th>
                                        <th>:</th>
                                        <td>@Model.CustomerPhone</td>
                                    </tr>
                                    <tr>
                                        <th>Email</th>
                                        <th>:</th>
                                        <td>@Model.CustomerEmail</td>
                                        <th>Address</th>
                                        <th>:</th>
                                        <td>@Model.CustomerAddress</td>
                                    </tr>
                                    <tr>
                                        <th>Created By</th>
                                        <th>:</th>
                                        <td>@Model.CreatedBy</td>
                                        <th>Create Date</th>
                                        <th>:</th>
                                        <td>@Model.CreatedDate</td>

                                    </tr>
                                    @if (Model.TransportTypeId == (int)TransportTypeEnum.Courier)
                                    {
                                        <tr>
                                            <th>Courier</th>
                                            <th>:</th>
                                            <td>@Model.DriverName - @Model.VehicleNo</td>
                                            <th>Courier Charge</th>
                                            <th>:</th>
                                            <td>@Model.Carrying</td>

                                        </tr>
                                    }
                                    else
                                    {
                                        <tr>
                                            <th>Truck</th>
                                            <th>:</th>
                                            <td>@Model.DriverName - @Model.DriverMobileNo - @Model.VehicleNo</td>
                                            <th>Truck Fair</th>
                                            <th>:</th>
                                            <td>@Model.Carrying</td>

                                        </tr>
                                    }

                                </table>
                            </div>
                            <div class="col-md-1"> </div>

                        }
                        else
                        {



                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="required">Challan Date</label>

                                        <input name="DeliveryDate" asp-for="DeliveryDate" type="text" value="@DateTime.Today.ToString("yyyy-MM-dd")" required="required" class="datePicker form-control" id="txtDeliveryDate" placeholder="Challan Date" autocomplete="off">

                                        @Html.ValidationMessageFor(model => model.DeliveryDate, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group" style="padding:0px">
                                        <label for="message-text" class="col-form-label required">Warehouse:</label>
                                        @Html.DropDownListFor(model => model.StockInfoId, Model.StockInfoList as SelectList, "==Warehouse==", new { id = "ddlCustomerId", @class = "form-control searchingDropdown", style = "width:100%", required = "required" })
                                        @Html.ValidationMessageFor(model => model.StockInfoId, "", new { @class = "text-danger" })
                                    </div>
                                </div>



                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group" style="padding:0px">
                                        <label class="required">Order Number</label>
                                        @Html.EditorFor(model => model.OrderNo, new { htmlAttributes = new { @autocomplete = "on", @class = "form-control", @name = "Order No", @id = "txtOrderNo" } })

                                        @Html.HiddenFor(model => model.OrderMasterId, new { id = "ddlOrderMasterId" })
                                        @Html.ValidationMessageFor(model => model.OrderMasterId, "", new { @class = "text-danger" })

                                    </div>

                                    <table id="orderDetailsTable" class="table">
                                        <tr>
                                            <td>Customer</td>
                                            <td>
                                                <span id="txtCustomerName"></span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Order Date
