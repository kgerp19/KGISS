    <DataSet Name="HederData">
      <Query>
        <DataSourceName>GeneralLedgerDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PurchaseOrderId">
            <Value>=Parameters!PurchaseOrderId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT purchaseOrder.PurchaseOrderId, 
vendor.Name VendorName,
vendor.Code VendorCode,
'Phone: ' + vendor.Phone + ' Email: ' + vendor.Email  VendorMobile,
vendor.Propietor VendorPropietor,
vendor.Address VendorAddress,
vendor.ContactName ContactName,
employee.Name EmployeeName,
employee.MobileNo EmployeeMobileNo,
designation.Name DesignationName,
purchaseOrder.PurchaseOrderNo,
 CONVERT(date, purchaseOrder.PurchaseDate) AS PurchaseDate ,
purchaseOrder.Remarks,
purchaseOrder.IsHold,
purchaseOrder.IsCancel,
purchaseOrder.Status,
purchaseOrder.SupplierPaymentMethodEnumFK,
purchaseOrder.TermsAndCondition,
purchaseOrder.CompanyId,
purchaseOrder.DeliveryAddress,
purchaseOrder.DeliveryDate, 
purchaseOrder.SupplierId,
purchaseOrder.FreightCharge,
purchaseOrder.OtherCharge,	
purchaseOrder.CreatedBy
FROM [Erp].[PurchaseOrder] purchaseOrder
join [Erp].Vendor vendor on purchaseOrder.SupplierId = vendor.VendorId
left join [dbo].Employee employee on purchaseOrder.EmpId = employee.Id
left  join [dbo].Designation designation on employee.DesignationId = designation.DesignationId

Where purchaseOrder.IsActive = 1 AND purchaseOrder.PurchaseOrderId = @PurchaseOrderId AND purchaseOrder.CompanyId = @CompanyId
                                                        </CommandText>
      </Query>
