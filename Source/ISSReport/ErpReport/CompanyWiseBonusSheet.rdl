    <DataSet Name="TimeDateDataSet">
      <Query>
        <DataSourceName>CompanyWiseSalarySheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PayRollId">
            <Value>=Parameters!PayRollId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>CREATE TABLE #TempMonthName(	 
	[MonthName] [nvarchar](50) NULL	 
 )
  INSERT INTO #TempMonthName
	 Select 
	
	CONCAT( DateName( month , DateAdd( month , pr.Month , 0 ) - 1 ), ', ', pr.Year) AS MonthName
  from PRoll_PayRoll pr
  where PayRollId = @PayRollId
  select x.MonthName from #TempMonthName x
  DROP Table #TempMonthName

  </CommandText>
      </Query>
