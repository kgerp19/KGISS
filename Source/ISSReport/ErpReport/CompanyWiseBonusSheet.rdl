    <DataSet Name="CompanyDset">
      <Query>
        <DataSourceName>CompanyWiseSalarySheetDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

SELECT      Name, Address,
                             (SELECT        Url
                               FROM            UrlInfo
                               WHERE        (UrlType = 1)) + 'Images/Logo/' + CompanyLogo AS CompanyLogo
FROM            Company
WHERE        (CompanyId = @CompanyId)</CommandText>
      </Query>
