    <DataSet Name="OtherExpDataSet">
      <Query>
        <DataSourceName>ProfitLossDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StrFromDate">
            <Value>=Parameters!StrFromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StrToDate">
            <Value>=Parameters!StrToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CostCenterId">
            <Value>=Parameters!CostCenterId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @FromDate Date,  @ToDate Date

select @FromDate=convert(varchar, convert(date, @StrFromDate, 105))
select @ToDate=convert(varchar, convert(date, @StrToDate, 105))



select h2.AccName,h2.ParentId OrderBy, IsNull(sum(Isnull(DebitAmount,0)) - sum(Isnull(CreditAmount,0)),0) as Balance from VoucherDetail vd
 join Voucher v on v.VoucherId=vd.VoucherId
 join HeadGL gl on vd.AccountHeadId=gl.Id
 join Head5 h5 on gl.ParentId=h5.Id
 join Head4 h4 on h5.ParentId=h4.Id
 join Head3 h3 on h4.ParentId =h3.Id
 join Head2 h2 on h3.ParentId =h2.Id
where h2.AccCode in ('43','44','45','46') and v.CompanyId= @CompanyId 
AND (@CostCenterId = 0  or v.Accounting_CostCenterFk = @CostCenterId)
and v.VoucherDate  between @FromDate and @ToDate
AND v.IsActive =1 AND vd.IsActive = 1
AND v.IsSubmit=1
Group by h2.AccName,h2.AccCode,h2.ParentId </CommandText>
      </Query>
      <Fields>
        <Field Name="AccName">
          <DataField>AccName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderBy">
          <DataField>OrderBy</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
