  <Code>Public Function SetBDCurrency(ByVal a As String) As String

        Dim info As New System.Globalization.CultureInfo("hi-IN")
        Dim s As String
        Dim value As Double
        Dim valueApplied As String
        value = Double.Parse(a)

        If value &lt; 0 Then
            Dim minusValue As Double
            Dim minusStr As String
	minusStr = value.ToString()
            minusValue = minusStr.Substring(1)            
            valueApplied = "(" &amp; minusValue.ToString("N2", info) &amp; ")"
        Else
            valueApplied = value.ToString("N2", info)
        End If
    Return valueApplied
    End Function</Code>
