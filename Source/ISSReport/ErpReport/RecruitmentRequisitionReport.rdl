    <DataSet Name="RequisitionDetails">
      <Query>
        <DataSourceName>GeneralLedgerDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select r.Id, rd.Id as RecruitmentApprovalId,rd.TargetSalary, 
rd.NumberOfVacancy, rd.JobLocation, rd.JobTitle, emp.Name as ManagerName,
 rd.JobDetails,rd.AdvertisementMedia, rd.ExpectedJoiningDate

from Erp.RecruitmentRequisitionDetails rd
join Erp.RecruitmentRequisition r on rd.RecruitmentRequisitionId = r.Id
join Employee emp on rd.ReportingManagerId = emp.Id 
join Company c on emp.CompanyId = c.CompanyId
join Department d on emp.DepartmentId = d.DepartmentId
join Designation ds on emp.DesignationId = ds.DesignationId
where r.Id = @id</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RecruitmentApprovalId">
          <DataField>RecruitmentApprovalId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TargetSalary">
          <DataField>TargetSalary</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NumberOfVacancy">
          <DataField>NumberOfVacancy</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="JobLocation">
