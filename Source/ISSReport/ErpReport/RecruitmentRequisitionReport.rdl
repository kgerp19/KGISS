        </Field>
        <Field Name="ExpectedJoiningDate">
          <DataField>ExpectedJoiningDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ApprovalList">
      <Query>
        <DataSourceName>GeneralLedgerDS</DataSourceName>
        <CommandText>select r.Id, approval.Id as ApprovalId, emp.Name as EmployeeName, c.Name as CompanyName,
ds.Name as DesignationName,
d.Name as DepartmentName

,CASE 
    WHEN approval.Status = 0 THEN 'Pending'
    WHEN approval.Status = 1 THEN 'Approved'
    ELSE 'Rejected'
   END AS ApprovalStatus,
approval.ModifiedDate as ApprovedTime
from Erp.RecruitmentRequisitionApproval approval
join Erp.RecruitmentRequisition r on approval.RecruitmentRequisitionId = r.Id
join Employee emp on approval.EmployeeId = emp.Id 
join Company c on emp.CompanyId = c.CompanyId
join Department d on emp.DepartmentId = d.DepartmentId
join Designation ds on emp.DesignationId = ds.DesignationId
where  approval.IsActive = 1 and r.Id = 14</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ApprovalId">
          <DataField>ApprovalId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EmployeeName">
          <DataField>EmployeeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
