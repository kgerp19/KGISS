          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyEmail">
          <DataField>CompanyEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="HederData">
      <Query>
        <DataSourceName>GeneralLedgerDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IssueMasterId">
            <Value>=Parameters!IssueMasterId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    t1.IssueMasterId,
    t6.Name AS CustomerName,
	t6.MobileNo AS CustomerContract,
    ISNULL(t4.Name, '') AS IssueBy,
    ISNULL(t4.MobileNo, '') AS EmployeeMobile,
    t1.CreatedBy,
    t1.Remarks,
    t1.IssueNo,
    t1.IssueDate,
    t1.CreatedDate,
    t5.Name AS AchknologeName,
    t1.AcknologeDate,
    t1.Achknolagement,
    t1.IsSubmit
FROM Erp.IssueMasterInfo t1
LEFT JOIN Employee t4 ON t1.IssuedBy = t4.Id
LEFT JOIN Employee t6 ON t1.VendorId = t6.Id
LEFT JOIN Employee t5 ON t1.AchknologeBy = t5.Id
WHERE t1.IsActive = 1 
    AND t1.IssueMasterId = @IssueMasterId 
    AND t1.CompanyId = @CompanyId</CommandText>
      </Query>
