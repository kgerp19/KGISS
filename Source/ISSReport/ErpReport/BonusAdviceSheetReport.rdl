    <DataSet Name="SalarySetupDataSet">
      <Query>
        <DataSourceName>EmpAttendanceDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PayRollId">
            <Value>=Parameters!PayRollId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select c.Name as CompanyName, p.Month,p.Year, p.Note, (SELECT DATENAME(MONTH, DATEADD(MONTH,p.Month - 1, '19000101'))) AS MonthName
from PRoll_PayRoll p
join Company c on p.CompanyId = c.CompanyId
where PayRollId = @PayRollId</CommandText>
      </Query>
