    <DataSet Name="BankDetail">
      <Query>
        <DataSourceName>EmpAttendanceDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BankBranchId">
            <Value>=Parameters!BankBranchId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select b.Name, bb.Name as BranchName, bb.AccountType, bb.AccountName, bb.AccountNumber, bb.Street, bb.City, bb.ZIPCode from BankBranch bb
join Bank b on bb.BankId = b.BankId
where BankBranchId = @BankBranchId</CommandText>
